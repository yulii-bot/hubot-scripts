{
  "name": "hosted-hubot",
  "version": "2.7.1",
  "private": true,
  "author": "GitHub Inc.",
  "keywords": [
    "github",
    "hubot",
    "campfire",
    "bot"
  ],
  "description": "A simple helpful robot for your Company",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/github/hubot/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/github/hubot.git"
  },
  "dependencies": {
    "david": "^7.0.0",
    "hubot": ">= 2.16.0 < 3.0.0",
    "hubot-heroku-keepalive": "^1.0.2",
    "hubot-scripts": ">= 2.16.0 < 3.0.0",
    "hubot-slack": ">= 3.3.0  < 4.0.0"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^1.9.2",
    "coffee-script": "^1.10.0",
    "coveralls": "^2.11.2",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": ">= 1.0.0 < 2.0.0",
    "hubot-mock-adapter": "^1.0.0"
  },
  "engines": {
    "node": ">= 0.8.x",
    "npm": ">= 1.1.x"
  },
  "config": {
    "blanket": {
      "pattern": [
        "hubot-scripts/scripts",
        "hubot-scripts/module"
      ],
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": "node_modules"
    }
  },
  "scripts": {
    "test": "mocha",
    "coveralls": "mocha test --require blanket --reporter mocha-lcov-reporter | coveralls"
  }
}
